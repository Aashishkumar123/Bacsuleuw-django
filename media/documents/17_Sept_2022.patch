 from django.shortcuts import render
 from django.contrib.auth.decorators import login_required
+from authentication.utils import User
+from django.http import JsonResponse
 
 
 @login_required()
 def index(request):
     return render(request, 'dashboard/corporate/index.html')
+
+
+def customer_listing(request):
+    users = User.objects.filter(is_customer=True)
+    context = {'users':users}
+    return render(request,'dashboard/corporate/customer-listing.html',context=context)
+
+
+def activate_customer(request):
+    if request.method == "POST":
+        email = request.POST.get('email')
+        active = User.objects.get(email=email)
+        active.is_active = True
+        active.save()
+        return JsonResponse({'status':200})
+
+
+def deactivate_customer(request):
+    if request.method == "POST":
+        email = request.POST.get('email')
+        inactive = User.objects.get(email=email)
+        inactive.is_active = False
+        inactive.save()
+        return JsonResponse({'status':200})
